<script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>

<link
  rel="stylesheet"
  id="bootstrap-css"
/>
<script src="//maxcdn.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js"></script>

<div id="instructor">
  <h3 class="text-center text-white pt-5">InstructorForm form</h3>
  <div class="container">
    <div
      id="instructorForm-row"
      class="row justify-content-center align-items-center"
    >
      <div id="instructorForm-column" class="col-md-6">
        <div id="instructorForm-box" class="col-md-12">
          <form id="instructorForm" class="form" action="/home" method="post">
            <h3 class="text-center text-info">Instructor Form</h3>
            <div class="form-group">
              <label for="firstName" class="text-info"
                >Instructor Full Name:</label
              ><br />
              <input
                type="text"
                name="name"
                id="firstName"
                class="form-control"
                required>
            </div>
            <div class="form-group">
              <label for="email" class="text-info"
                >Instructor Email:</label
              ><br />
              <input
                type="text"
                name="email"
                id="email"
                class="form-control"
                required>
            </div>

            <div class="form-group">
              <label for="password" class="text-info"
              >Instructor Password:</label
              ><br />
              <input
                      type="password"
                      name="password"
                      id="password"
                      class="form-control"
                      required>
            </div>

            <div class="form-group">
              <input
                      type="submit"
                      name="submit"
                      class="btn btn-info btn-md"
                      value="Create Instructor Account"
              />
            </div>

          </form>
        </div>
      </div>
    </div>
  </div>
</div>
