<div class="row justify-content-center align-items-center">
  <div id="instructorForm-column" class="col-md-6">
    <div id="instructorForm-box" class="col-md-12">
      <form id="instructorForm" class="form" action="/instructor" method="POST">
        <h3 class="text-center text-info">Welcome Professor <%=newInstructor%></h3>

<!--        create class-->
        <div class="form-group">
          <h5 class="text-info">Create a new class: </h5>

          <label for="className" class="text-info">Class Name:</label><br />
          <input
                  type="text"
                  name="className"
                  id="className"
                  class="form-control"
                  required>
        </div>
        <div class="form-group">
          <label for="selectDay" class="text-info">Select the Semester:</label
          ><br />
          <select name="year" class="right"  required>
            <option value="Spring selected">Spring</option>
            <option value="Summer">Summer</option>
            <option value="Fall">Fall</option>

          </select>
        </div>
        <div class="form-group">
          <label for="selectDay" class="text-info">Select the year:</label
          ><br />
          <select name="year" class="right"  required>
            <option value="2021 selected">2021</option>
            <option value="2022">2022</option>
            <option value="2023">2023</option>
            <option value="2024" >2024</option>
            <option value="2025">2025</option>
          </select>
        </div>
        <div class="form-group">
          <label for="selectDay" class="text-info">Select the day:</label
          ><br />
          <select name="weekday" class="right" multiple required>
            <option value="Monday" selected>Monday</option>
            <option value="Tuesday">Tuesday</option>
            <option value="Wednesday">Wednesday</option>
            <option value="Thursday">Thursday</option>
            <option value="Friday" >Friday</option>
            <option value="Saturday">Saturday</option>
            <option value="Sunday">Sunday</option>
          </select>
        </div>
        <div class="form-group">
          <label for="classTime" class="text-info">Class Start Time:</label
          ><br />
          <input
                  type="time"
                  name="startTime"
                  id="startTime"
                  class="form-control"
                  required>
        </div>
        <div class="form-group">
          <label for="classTime" class="text-info">Class End Time:</label
          ><br />
          <input
                  type="time"
                  name="endTime"
                  id="endTime"
                  class="form-control"
                  required>
        </div>
        <div class="form-group">
          <label for="password" class="text-info">Class password:</label><br />
          <input
                  type="password"
                  name="password"
                  id="password"
                  class="form-control"
                  required>
        </div>
        <div class="form-group">
          <input
                  type="submit"
                  name="submit"
                  class="btn btn-info btn-md"
                  value="Generate Link"
          />
        </div>


        <button
                type="submit"
                name="submit"
                class="btn btn-info btn-md"
                value="Submit"
        >Register</button>
        <div id="login-link" class="">
          <a href="/login?classID=" class="text-info">I am the instructor!</a>
        </div>
        <div>If you already have an account, you can <a href="/login">Log in.</a></div>
        <div>Return to <a href="/">Home Page.</a></div>

      </form>
    </div>
  </div>

</div>

